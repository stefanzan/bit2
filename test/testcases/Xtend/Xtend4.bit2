«var paragraphs=[{headline:"Greeting", text:"Good Morning"}, {headline:"Weather", text:"Sunny"}]»
<html>
  <body>
  «for p in paragraphs front "<div>" rear "</div>" separator "</div><div>"»
    «if p.headline != ""»
    <h1>«p.headline»</h1>
    «endif»
    <p>
    «p.text»
    </p>
  «endfor»
  </body>
</html>